// sets in JS



const mySet = new Set();
console.log('the set looks like',mySet);


// adding values to this set
mySet.add('this');
mySet.add('that');
mySet.add('this');
mySet.add(34);
mySet.add(34);
mySet.add(true);
mySet.add(false);
console.log(mySet)
// set is useful for making sure our collection handles unique values


